/* Style Standards & typography */
@import 'settings'

$golden-ratio = 1.61803398875

@font-face
  font-family $font-family
  font-style normal

/* Reset font-family */
h1, h2, h3, h4, h5, h6
  font-family $font-family
  color $font-color

p, div
  font-family $font-family
  color $font-color-light
  line-height $golden-ratio

