{
  "name": "sabor-website",
  "version": "2.0.0",
  "description": "Sabor: Columbia University's First Latino Dance Troupe",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run dev:watch",
    "tests": "npm run test && npm run lint",

    "deploy": "npm run test && npm run lint && npm run prod:build && sh ./deploy.sh",

    "dev:watch": "cross-env NODE_ENV=development npm run watch",
    "dev:build": "cross-env NODE_ENV=development npm run build",
    "prod:watch": "cross-env NODE_ENV=production npm run watch",
    "prod:build": "cross-env NODE_ENV=production npm run build",

    "watch": "npm run clean && gulp",
    "build": "npm run clean && gulp build",

    "test": "gulp test",
    "lint": "gulp lint",

    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunyang713/sabor-website.git"
  },
  "author": "Jonathan Sun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunyang713/sabor-website/issues"
  },
  "homepage": "https://github.com/sunyang713/sabor-website#readme",
